<configuration>
  <system.diagnostics>
    <trace autoflush="false" indentsize="4">
      <listeners>
        <remove name="Default" />
        <add name="myListener"  type="System.Diagnostics.TextWriterTraceListener"    initializeData="myListener.log" />
      </listeners>
    </trace>
  </system.diagnostics>
</configuration>
